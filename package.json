{
  "name": "hueneu-website",
  "version": "0.1.0",
  "private": true,
  "description": "A one-page responsive website for hueneu graphic design studio.",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "react-scripts build",
    "dev:client": "react-scripts start",
    "dev:server": "nodemon server/index.js --watch server",
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "test": "react-scripts test"
  },
  "dependencies": {
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "react-scripts": "5.0.1",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
